{
  "name": "college-chatbot-backend",
  "version": "1.0.0",
  "description": "Backend for AI-powered College ChatBot",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev src/server.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "seed": "ts-node src/utils/db/seed.ts",
    "migrate": "ts-node src/utils/db/migrate.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "swagger": "ts-node src/docs/swaggerSetup.ts"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.4.3",
    "morgan": "^1.10.0",
    "winston": "^3.10.0",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.5.1",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "express-rate-limit": "^6.7.0",
    "jest": "^29.6.2",
    "joi": "^17.13.3",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^4.6.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
